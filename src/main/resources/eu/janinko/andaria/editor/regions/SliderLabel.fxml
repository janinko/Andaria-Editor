<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"  fx:controller="eu.janinko.andaria.editor.regions.SliderLabelController">
   <children>
      <CheckBox fx:id="checkbox" mnemonicParsing="false" onAction="#checkboxChange" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </HBox.margin></CheckBox>
      <Slider fx:id="slider" disable="true" majorTickUnit="1.0" minorTickCount="0" snapToTicks="true" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </HBox.margin></Slider>
      <Label fx:id="label" alignment="CENTER" minWidth="30.0" text="---" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </HBox.margin>
      </Label>
   </children>
</HBox>
